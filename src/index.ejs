  <!-- Shared dependencies go into this import map -->
  <script type="importmap">
    {
      "imports": {
        "react": "https://esm.sh/react@18.2.0",
        "react-dom": "https://esm.sh/react-dom@18.2.0", 
        "single-spa": "https://cdn.jsdelivr.net/npm/single-spa@6.0.3/+esm",
